fn resta(a:int64, b:int64):int64
{
    return a-b;
}

fn main():void
{
    ini := 10;
    var fin:int = -10;

    for ini > fin 
    {
        ini = resta(ini, 1);
        println("ini: %d", ini);
    }
    println("adios!!");
} 
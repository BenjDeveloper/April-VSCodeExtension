{
    "name": "april",
    "displayName": "April",
    "description": "Syntax highlighting for April in VSCode",
    "version": "0.0.1",
    "publisher": "April Syntax",
    "icon": "./images/logo(10).png",
    "author": {
        "name": "by kenshin and Pandicorn"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PandiCornDeveloper/April-VSCodeExtension.git"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "april",
            "aliases": ["April", "april"],
            "extensions": [".april"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "april",
            "scopeName": "source.april",
            "path": "./syntaxes/april.tmLanguage.json"
        }],
        "iconThemes": [
            {
                "id": "iconFile",
                "label": "iconFile",
                "path": "./syntaxes/april.tmTheme.json"
            }
        ]
    }
}